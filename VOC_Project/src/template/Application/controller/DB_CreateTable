  CREATE TABLE "VOCPRO"."MOVIES" 
   (   "MOVIES_ID" NUMBER NOT NULL ENABLE, 
   "TITLE" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "GENRE" NUMBER NOT NULL ENABLE, 
   "DIRECTOR" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "AGE_GROUP" NUMBER NOT NULL ENABLE, 
   "STORY" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "AVERAGE_SCORE" NUMBER NOT NULL ENABLE, 
   "CHARACTER" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "OPEN_DATE" DATE NOT NULL ENABLE, 
   "PRODUCTION" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "IMAGE_FILE_NAME" NUMBER NOT NULL ENABLE, 
   "SCHEDULE_DATE" VARCHAR2(20 BYTE), 
   "RUNNING_TIME" VARCHAR2(20 BYTE)
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 NOCOMPRESS LOGGING
  STORAGE(INITIAL 65536 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1 BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SYSTEM" ;

   COMMENT ON COLUMN "VOCPRO"."MOVIES"."MOVIES_ID" IS '영화인덱스';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."TITLE" IS '영화제목';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."GENRE" IS '영화장르';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."DIRECTOR" IS '영화감독';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."AGE_GROUP" IS '연령대';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."STORY" IS '줄거리';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."AVERAGE_SCORE" IS '영화평균별점';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."CHARACTER" IS '등장인물';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."OPEN_DATE" IS '개봉일자';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."PRODUCTION" IS '제조사';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."IMAGE_FILE_NAME" IS '이미지파일명';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."SCHEDULE_DATE" IS '상영날짜:시간';
   COMMENT ON COLUMN "VOCPRO"."MOVIES"."RUNNING_TIME" IS '러닝타임';



  CREATE TABLE "VOCPRO"."MEMBER" 
   (   "MEMBER_ID" NUMBER NOT NULL ENABLE, 
   "ID" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "PASSWORD" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "NAME" VARCHAR2(20 BYTE) NOT NULL ENABLE, 
   "GENDER" NUMBER NOT NULL ENABLE, 
   "PHONE_NUMBER" NUMBER NOT NULL ENABLE, 
